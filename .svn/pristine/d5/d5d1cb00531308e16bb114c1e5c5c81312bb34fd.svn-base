.start-station{
	padding: 0.12rem;
	color: #777777;
	max-width: 0.85rem;
	overflow: hidden;
	white-space:nowrap; 
	text-overflow:ellipsis;
}
.start-station input{
	width: 90%;
	border: none;
}
.start-station:after{
	content: "";
  display: inline-block;
  position: relative;
  width: 0.06rem;
  height: 0.04rem;
  background-image: url(../../static/images/icon.png);
  background-size: 3.755rem 3.415rem;
  background-repeat: no-repeat;
  background-position: -0.67rem -0.66rem;
  top: -0.02rem;
  left: -0.1rem;
}
.jiantou{
	width: 0.14rem;
	height: 0.05rem;
	background-image: url(../../static/images/icon.png);
	background-size: 3.755rem 3.415rem;
	background-repeat: no-repeat;
	background-position: -0.83rem -0.66rem;
	position: relative;
	top: -0.02rem;
}
.jiantou2{
	width: 0.14rem;
	height: 0.05rem;
	background-image: url(../../static/images/icon.png);
	background-size: 3.755rem 3.415rem;
	background-repeat: no-repeat;
	background-position: -0.83rem -0.66rem;
	margin-top: 0.08rem;
	margin-bottom: 0.08rem;
}
.end-station{
	padding: 0.12rem;
	color: #777777;
	max-width: 0.85rem;
	overflow: hidden;
	white-space:nowrap; 
	text-overflow:ellipsis;
	
}
.end-station input{
	width: 90%;
	border: none;
}
.end-station:after{
	content: "";
  display: inline-block;
  position: relative;
  width: 0.06rem;
  height: 0.04rem;
  background-image: url(../../static/images/icon.png);
  background-size: 3.755rem 3.415rem;
  background-repeat: no-repeat;
  background-position: -0.67rem -0.66rem;
  top: -0.02rem;
  left: -0.1rem;
}
.price{
	margin: 0.12rem 0;
	padding: 0 0.12rem;
	color: #777777;
	border-left: 1px solid #ededed;
	max-width: 0.85rem;
	overflow: hidden;
	white-space:nowrap; 
	text-overflow:ellipsis;
}
.price:after{
	content: "";
  display: inline-block;
  position: relative;
  width: 0.06rem;
  height: 0.04rem;
  background-image: url(../../static/images/icon.png);
  background-size: 3.755rem 3.415rem;
  background-repeat: no-repeat;
  background-position: -0.67rem -0.66rem;
  top: -0.02rem;
  margin-left: 0.05rem;
}
.length{
	max-width: 0.85rem;
	overflow: hidden;
	white-space:nowrap; 
	text-overflow:ellipsis;
	margin: 0.12rem 0;
	padding: 0 0.12rem;
	color: #777777;
	border-left: 1px solid #ededed;
	
}
.length:after{
	content: "";
  display: inline-block;
  position: relative;
  width: 0.06rem;
  height: 0.04rem;
  background-image: url(../../static/images/icon.png);
  background-size: 3.755rem 3.415rem;
  background-repeat: no-repeat;
  background-position: -0.67rem -0.66rem;
  top: -0.02rem;
  margin-left: 0.05rem;
}
.man-info>img{
	width: 0.2rem;
	height: 0.2rem;
}
.phone{
	width: 0.21rem;
	height: 0.21rem;
	background-image: url(../../static/images/icon.png);
  background-size: 3.755rem 3.415rem;
  background-repeat: no-repeat;
  background-position: -1rem -0.86rem;
}
.price-label{
	color: #45C175;
	border: 1px solid #45C175;
	border-radius: 0.04rem;
	padding: 0.03rem;
}
.goods-info>span{
	margin-right: 0.03rem;
}
.bor-bottom{
	border-bottom: 1px solid #ededed;
}
.release{
	font-size: 0.25rem;
	color: #45C175;
}
.price-alert{
	bottom: 0;
	left: 0;
	right: 0;
	background: rgba(0,0,0,0.5);
	z-index: 20;
	display: none;
}
.length-alert{
	bottom: 0;
	left: 0;
	right: 0;
	background: rgba(0,0,0,0.5);
	z-index: 20;
	display: none;
}
.around{
	border: 1px solid #ededed;
	border-radius: 0.03rem;
}
.length-sel{
	width: 0.72rem;
	margin: 0.07rem;
	height: 0.3rem;
	line-height: 0.28rem;
	text-align: center;
	border: 1px solid #ededed;
	border-radius: 0.03rem;
}
.else-length{
	margin: 0.07rem;
	border: 1px solid #ededed;
	width: 1.59rem;
	border-radius: 0.03rem;
	font-size: 0.14rem;
	padding: 0.07rem 0.1rem;
}
#submit{
	background-color: #45C175;
	color: #FFFFFF;
	padding: 0.1rem;
	font-size: 0.14rem;
	border-radius: 0.04rem;
	margin: 0.07rem;
	margin-top: 0.26rem;
}
.select{
	border: 1px solid #45C175!important;
	color: #45C175!important;
}
.submit-box{
	background-color: #ffffff;
	position: fixed;
	width: 100%;
	bottom: 0;
}
.bg-submit{
	background-color: #45C175;
}