.search{
	background-color: #f5f5f5;
	padding: 0.09rem;
	border-radius: 0.04rem;
	color: #999999;
}
.search:before{
	content: "";
	display: block;
	width: 0.14rem;
	height: 0.14rem;
	background-image: url(../../static/images/icon.png);
  background-size: 3.755rem 3.415rem;
  background-repeat: no-repeat;
  background-position: -0.37rem -0.45rem;
  margin-right: 0.1rem;
}
.xiala:after{
	content: "";
	display: block;
	width: 0.06rem;
	height: 0.04rem;
	background-image: url(../../static/images/icon.png);
  background-size: 3.755rem 3.415rem;
  background-repeat: no-repeat;
  background-position: -0.67rem -0.66rem;
  margin-right: 0.1rem;
  margin-left: 0.05rem;
}
.warehouse-img{
	width: 0.9rem;
	height: 0.77rem;
}
.cangku-label{
	color: #45C175;
	border: 1px solid #45C175;
	border-radius: 0.02rem;
	font-size: 0.1rem;
	padding: 0.025rem;
}
