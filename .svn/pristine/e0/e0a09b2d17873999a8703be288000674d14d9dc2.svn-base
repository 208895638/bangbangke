.start-station:before{
	content: "";
	display: inline-block;
	width: 0.05rem;
	height: 0.05rem;
	background-color: #45C175;
	border-radius: 50%;
	margin-right: 0.05rem;
	position: relative;
	top: -0.02rem;
}
.end-station:before{
	content: "";
	display: inline-block;
	width: 0.05rem;
	height: 0.05rem;
	background-color: #0056FF;
	border-radius: 50%;
	margin-right: 0.05rem;
	position: relative;
	top: -0.02rem;
}
.sanjiao{
	width: 0.1rem;
	height: 0.1rem;
	background-image: url(../../static/images/icon.png);
  background-size: 3.755rem 3.415rem;
  background-repeat: no-repeat;
  background-position: -0.54rem -0.66rem;
}
.bor-bottom{
	border-bottom: 1px solid #EDEDED;
}
input{
	padding: 0!important;
	border: 0!important;
}
.site{
	width: 100%;
}
.write-price{
	margin-left: 0.25rem;
	border: 1px solid #979797;
	border-radius: 0.04rem;
	width: 0.87rem;
	padding: 0.07rem 0.1rem;
	color: #cccccc;
	
}
.write-price>input{
	width: 100%;
}
.submit{
	box-shadow: 0 0 0.12rem rgba(33,33,33,0.08);
	padding: 0.1rem 0.2rem;
}
.submit>div{
	text-align: center;
  padding: 0.13rem;
  color: #ffffff;
  background-color: #45C175;
  border-radius: 0.04rem;
  font-size: 0.15rem;
}
.radio-sel{
	height: 0.18rem;
	width: 0.18rem;
	background-image: url(../../static/images/artboard.png);
	background-repeat: no-repeat;
	background-size: 0.47rem 0.18rem;
	margin-right: 0.1rem;
}
.radio-none{
	height: 0.18rem;
	width: 0.18rem;
	background-image: url(../../static/images/artboard.png);
	background-repeat: no-repeat;
	background-size: 0.47rem 0.18rem;
	background-position: -0.19rem 0;
	margin-right: 0.1rem;
}
.width100{
	width: 100%;
}
