.search{
	background-color: #f5f5f5;
	padding: 0.09rem;
	border-radius: 0.04rem;
	color: #999999;
}
.search:before{
	content: "";
	display: block;
	width: 0.14rem;
	height: 0.14rem;
	background-image: url(../../static/images/icon.png);
  background-size: 3.755rem 3.415rem;
  background-repeat: no-repeat;
  background-position: -0.37rem -0.45rem;
  margin-right: 0.1rem;
}
.xiala:after{
	content: "";
	display: block;
	width: 0.06rem;
	height: 0.04rem;
	background-image: url(../../static/images/icon.png);
  background-size: 3.755rem 3.415rem;
  background-repeat: no-repeat;
  background-position: -0.67rem -0.66rem;
  margin-right: 0.1rem;
  margin-left: 0.05rem;
}
#site:before{
  content: "";
  display: block;
  width: 0.11rem;
  height: 0.13rem;
  background-image: url(../../static/images/icon.png);
  background-size: 3.755rem 3.415rem;
  background-repeat: no-repeat;
  background-position: -1.66rem -0.25rem;
  margin-left: 0.15rem;
  margin-right: 0.05rem;
}
#site:after{
	content: "";
	display: block;
	width: 0.06rem;
	height: 0.04rem;
	background-image: url(../../static/images/icon.png);
  background-size: 3.755rem 3.415rem;
  background-repeat: no-repeat;
  background-position: -0.67rem -0.66rem;
  margin-right: 0.1rem;
  margin-left: 0.05rem;
}
.car-img{
	width: 0.9rem;
	height: 0.74rem;
}
.car-label{
	color: #F54725;
	border: 1px solid #F54725;
	border-radius: 0.02rem;
	padding: 0.01rem;
	font-size: 0.1rem;
}
.hide{
	display: none;
}
.box{
	position: fixed;
	bottom: 0;
	left: 0;
	right: 0;
	background: rgba(0,0,0,0.5);
	z-index: 20;
}
.searchCondations{
	border: none;
	background: transparent;
	outline: none;
}
